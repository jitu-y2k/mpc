@using WebApplication1.Models;
@model List<Student>
@{
//Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
    <button onclick="">New Student</button>
    <a href="/Student/Create">New Student</a>
    
    <div>
        <table class="table table-striped table-active">
            <thead>
                <tr>
                    <th>Student Id</th>
                    <th>Student Name</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var student in Model)
                {
                    <tr>
                        <td>@student.Id</td>
                        <td>@student.Name</td>
                    </tr>
                    
                }
            </tbody>
        </table>
    </div>
</body>
</html>
